<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<Pane xmlns:fx="http://javafx.com/fxml/1" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/9.0.1"
      fx:controller="main.java.drawie.MenuController">
    <children>
        <Button layoutX="363.0" layoutY="285.0" mnemonicParsing="false" onMouseClicked="#createNewRoom" text="New Room"
                textAlignment="CENTER"/>
        <Button layoutX="364.0" layoutY="319.0" mnemonicParsing="false" onMouseClicked="#joinExistingRoom"
                text="Join Room"/>
        <TextField fx:id="roomUrl" layoutX="218.0" layoutY="354.0" prefHeight="25.0" prefWidth="365.0"/>
        <Text layoutX="259.0" layoutY="246.0" strokeType="OUTSIDE" strokeWidth="0.0" text="DRAWIE!">
            <font>
                <Font name="Arial Italic" size="65.0"/>
            </font>
        </Text>
        <Button layoutX="362.0" layoutY="390.0" mnemonicParsing="false" onMouseClicked="#exitDrawie" prefHeight="25.0"
                prefWidth="72.0" text="Exit" textAlignment="CENTER"/>
    </children>
</Pane>
